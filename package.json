    "git-clone": "git clone [リポジトリアドレス] [配置するディレクトリパス(未指定の場合はリポジトリ名と同じ)]",
    "git-pull": "git pull",
    "watch": "gulp watch",
    "imagemin": "gulp imagemin",
    "js": "gulp js",
    "lint": "sass-lint-auto-fix -c scss-lint.yml & gulp lint"
