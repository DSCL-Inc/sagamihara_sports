    "server": "php -S localhost:8000 -t ./site/",
    "deploy": "node deploy",
    "git-add": "git add .",
    "git-comit": "git commit",
    "git-remote-add": "git remote add origin https://yousukeasami@bitbucket.org/triandinc/nishinokoumuten.git",
